{
    "name": "jpbernius/fmeat",
    "description": "PHP Library for accessing the FMeat API",
    "keywords": ["FMeat","TUM"],
    "homepage": "https://jpbernius.github.io/FMeat.php/",
    "license": "MIT",
    "authors": [
        {
            "name": "Jan Philip Bernius",
            "email": "janphilip@bernius.net",
            "homepage": "https://janphilip.bernius.net"
        }
    ],
    "require": {
        "php": ">=7.1.0",
        "guzzlehttp/guzzle": "~6.0",
        "php-di/php-di": "^6.0",
        "tedivm/stash": "^0.15.1"
    },
    "require-dev": {
        "phpunit/phpunit": "~8.0",
        "squizlabs/php_codesniffer": "~3.4",
        "hamcrest/hamcrest-php": "^2.0"
    },
  "scripts": {
    "test": "phpunit",
    "clover": "phpunit --coverage-clover=coverage.clover"
  },
  "autoload": {
    "psr-4": {
      "JPBernius\\FMeat\\": "src/JPBernius/FMeat/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "JPBernius\\FMeat\\": "test/JPBernius/FMeat/"
    }
  }
}
